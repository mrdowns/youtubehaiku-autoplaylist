{
  "manifest version": 2,
  "name": "/r/youtubehaiku auto playlist",
  "version": "0.1",
  "default_locale": "en",
  "description": "plays videos from /r/youtubehaiku above a certain point threshold",
  "borwser_action": {
    "default_icon": "icon.png",
  },
  "permissions": [
    "activeTab", "https://www.reddit.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://www.reddit.com/*"],
      "js": ["findvideos.js"]
    }
  ]
}
